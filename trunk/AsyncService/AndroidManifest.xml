<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.devtcg.asyncservice">
    <application android:theme="@android:style/Theme.Dark" android:icon="@drawable/icon">
        <activity class=".AsyncClient" android:label="@string/app_name">
            <intent-filter>
                <action android:value="android.intent.action.MAIN" />
                <category android:value="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service class=".AsyncService" android:process=":remote" />
    </application>
</manifest> 
